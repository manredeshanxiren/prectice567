#include<stdio.h>
int main()
{
    int blank, digit, i, other;
    char ch;

    blank = digit = other = 0;
    for (i = 1; i <= 15; i++)
    {
        ch = getchar();
        switch (ch)
        {
            case ' ':
                blank++; break;
            case '\n':
                blank++; break;
            case '0':
                digit++; break;
            case '1':
                digit++; break;
            case '2':
                digit++; break;
            case '3':
                digit++; break;
            case '4':
                digit++; break;
            case '5':
                digit++; break;
            case '6':
                digit++; break;
            case '7':
                digit++; break;
            case '8':
                digit++; break;
            case '9':
                digit++; break;
            default:
                other++; break;
        }
    }
    printf_s("blank = %d, digit = %d, other = %d\n", blank, digit, other);
    return 0;
}
