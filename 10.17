#include <stdio.h>

#define MAXN 10

int Sum ( int List[], int N )
{
    int i = 0;
    int Sum = 0;
    for (i = 0; i < N; i++)
    {
        Sum += List[i];
    }
    return Sum;
}

int main ()
{
    int List[MAXN], N, i;

    scanf("%d", &N);
    for ( i=0; i<N; i++ )
        scanf("%d", &List[i]);
    printf("%d\n", Sum(List, N));

    return 0;
}

