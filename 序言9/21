#include <stdio.h>
double fact( int n ){
    int i;
    double result = 1.0;
    for(i=1; i<=n; i++)
        result = result * i;
    return result;
}
  void main()
  {
  	int n,i;
  	double result;
  	scanf("%d",&n);
  	for(i=0;i<=n;i++){
  	  result=fact(i);
	  printf("%d! = %.0f\n",i,result); 
  		
	  }
   } 
